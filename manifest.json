{
  "manifest_version": 3,
  "name": "Upwork Optimizer",
  "description": "Toggle the announcement banner on & off on the Find Work page",
  "version": "1.01",
  "content_scripts": [
    {
      "matches": ["*://*.upwork.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
